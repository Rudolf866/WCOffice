CREATE TABLE `iwater_moved_orders` (
	`order_id` INT(10) NULL DEFAULT NULL,
	`done` INT(1) NULL DEFAULT '0',
	`reason` VARCHAR(255) NULL DEFAULT NULL,
	`comment` VARCHAR(255) NULL DEFAULT NULL,
	`agreed` INT(2) NULL DEFAULT NULL,
	`server_time` VARCHAR(50) NULL DEFAULT NULL,
	`driver_coords` VARCHAR(255) NULL DEFAULT NULL
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;
